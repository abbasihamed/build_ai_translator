{
    "manifest_version": 3,
    "name": "AI Translator",
    "version": "1.0",
    "description": "A Flutter-based Chrome extension",
    "action": {
      "default_popup": "index.html",
      "default_icon": "favicon.png"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    }
  }
  